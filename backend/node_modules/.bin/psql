#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/e/swado-tech-assig/backend/node_modules/.pnpm/psql@0.0.1/node_modules/psql/bin/node_modules:/mnt/e/swado-tech-assig/backend/node_modules/.pnpm/psql@0.0.1/node_modules/psql/node_modules:/mnt/e/swado-tech-assig/backend/node_modules/.pnpm/psql@0.0.1/node_modules:/mnt/e/swado-tech-assig/backend/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/mnt/e/swado-tech-assig/backend/node_modules/.pnpm/psql@0.0.1/node_modules/psql/bin/node_modules:/mnt/e/swado-tech-assig/backend/node_modules/.pnpm/psql@0.0.1/node_modules/psql/node_modules:/mnt/e/swado-tech-assig/backend/node_modules/.pnpm/psql@0.0.1/node_modules:/mnt/e/swado-tech-assig/backend/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../.pnpm/psql@0.0.1/node_modules/psql/bin/psql.js" "$@"
else
  exec node  "$basedir/../.pnpm/psql@0.0.1/node_modules/psql/bin/psql.js" "$@"
fi
